                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <font>
                  <Font name="Monospaced Regular" size="18.0" />
               </font>
               <GridPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </GridPane.margin>
            </TextArea>
            <HBox alignment="CENTER_LEFT" spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label fx:id="labelName" text="labelPlayerName" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <font>
                        <Font name="Monospaced Regular" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="labelHealth" text="labelPlayerHealth">
                     <font>
                        <Font name="Monospaced Regular" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="labelWeapon" text="labelPlayerWeapon">
                     <font>
                        <Font name="Monospaced Regular" size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Label fx:id="labelTime" text="labelTime" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
               <font>
                  <Font name="Monospaced Regular" size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnEnter" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#startGame" text="ENTER" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <font>
                  <Font name="Monospaced Regular" size="36.0" />
               </font>
            </Button>
            <Button fx:id="btnContinue" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleContinue" text="Continue" visible="false" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <font>
                  <Font name="Monospaced Regular" size="36.0" />
               </font>
            </Button>
            <HBox fx:id="hboxInventory" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </GridPane.margin>
            </HBox>
