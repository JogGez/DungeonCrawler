                        <Font name="Monospaced Regular" size="13.0" />
                     </font></Button>
                  <Button fx:id="btnTalk" mnemonicParsing="false" onAction="#handleTalk" text="Talk" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
                     <font>
                        <Font name="Monospaced Regular" size="13.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </GridPane>
            <TextArea fx:id="textAreaMap" editable="false" promptText="Map" GridPane.columnIndex="2" GridPane.rowIndex="2">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <font>
                  <Font name="Monospaced Regular" size="18.0" />
               </font>
               <GridPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </GridPane.margin>
            </TextArea>
            <TextArea fx:id="textAreaInventory" editable="false" promptText="Inventory" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="3">
