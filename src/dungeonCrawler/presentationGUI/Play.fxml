            <ColumnConstraints hgrow="NEVER" maxWidth="1060.0" minWidth="10.0" percentWidth="5.0" prefWidth="30.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" percentWidth="60.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" percentWidth="30.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="536.0" minWidth="10.0" percentWidth="5.0" prefWidth="30.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="723.0" minHeight="10.0" percentHeight="5.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints maxHeight="723.0" minHeight="10.0" percentHeight="5.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="65.0" vgrow="NEVER" />
            <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="25.0" vgrow="NEVER" />
            <RowConstraints maxHeight="363.0" minHeight="-Infinity" percentHeight="5.0" prefHeight="30.0" vgrow="NEVER" />
         </rowConstraints>
         <children>
            <TextArea fx:id="textAreaMain" editable="false" promptText="Main" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font name="Monospaced Regular" size="16.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <GridPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </GridPane.margin>
            </TextArea>
            <GridPane fx:id="buttonGridPane" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button fx:id="btnUp" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleMove" prefHeight="25.0" prefWidth="50.0" text="Up" GridPane.columnIndex="1">
                     <font>
                        <Font name="Monospaced Regular" size="12.0" />
                     </font></Button>
                  <Button fx:id="btnDown" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleMove" prefHeight="25.0" prefWidth="50.0" text="Down" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <font>
                        <Font name="Monospaced Regular" size="12.0" />
                     </font></Button>
                  <Button fx:id="btnRight" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleMove" prefHeight="25.0" prefWidth="50.0" text="Right" GridPane.columnIndex="2" GridPane.rowIndex="1">
                     <font>
                        <Font name="Monospaced Regular" size="12.0" />
                     </font></Button>
                  <Button fx:id="btnLeft" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleMove" prefHeight="25.0" prefWidth="50.0" text="Left" GridPane.rowIndex="1">
                     <font>
                        <Font name="Monospaced Regular" size="12.0" />
                     </font></Button>
                  <Button fx:id="btnAttack" mnemonicParsing="false" onAction="#handleAttack" text="Attack" GridPane.halignment="CENTER">
                     <font>
                        <Font name="Monospaced Regular" size="13.0" />
                     </font></Button>
                  <Button fx:id="btnFlee" mnemonicParsing="false" onAction="#handleFlee" text="Flee" GridPane.columnIndex="2" GridPane.halignment="CENTER">
                     <font>
                        <Font name="Monospaced Regular" size="13.0" />
                     </font></Button>
                  <Button fx:id="btnOpen" mnemonicParsing="false" onAction="#handleOpen" text="Open" GridPane.halignment="CENTER" GridPane.rowIndex="2">
                     <font>
                        <Font name="Monospaced Regular" size="13.0" />
                     </font></Button>
                  <Button fx:id="btnSkip" mnemonicParsing="false" onAction="#handleSkip" text="Skip" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2">
